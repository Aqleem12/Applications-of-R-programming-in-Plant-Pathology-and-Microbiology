getwd()
setwd("D:/R")
Gary=read.csv(file="ViruesSpecies.csv",header=T)
Gary<- as(split(Gary$Viruses, Gary$id),"transactions")
summary(Gary)
rules <- apriori(Gary, parameter=list(support=0.1,confidence=0.5))
inspect(rules)
ordered_groceryrules <- sort(rules, by='lift',decreasing = F)  #decreasing = F,lift
inspect(ordered_groceryrules)
subrules2 <- head(ordered_groceryrules, 97)
inspect(subrules2)
plot(subrules2, method = "grouped",control = list(k = 97))
plot(subrules2, method = "grouped",control = list(k = 14))
plot(subrules2, method = "grouped",control = list(k = 97))
plot(subrules2, method = "graph",control = list(k = 97))
plot(subrules2, method = "graph",control = list(k = 10))
plot(subrules2, method = "graph",control = list(k = 10))
ordered_groceryrules <- sort(rules, by='lift',decreasing = T)  #decreasing = T,lift
inspect(ordered_groceryrules)
subrules2 <- head(ordered_groceryrules, 10)
inspect(subrules2)
plot(subrules2, method = "graph",control = list(k = 10))
